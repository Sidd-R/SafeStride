{
  "expo": {
    "name": "Safe Stride",
    "slug": "safestride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SafeStrideLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SafeStrideLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SafeStrideLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.siddhantrao.safestride"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "plugins": [
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
          }
        ]
      ]
    },
    "extra": {
      "eas": {
        "projectId": "d6a59235-70e4-4498-b146-c16c26a056e5"
      }
    }
  }
}
